@using Yeni.DTO
@model List<UrunDTO>


    <table class="table table-bordered">
    <tr>
        <th>UrunAdi</th>   
        <th>Urun Miktari</th>
        <th>Urun Açıklaması</th>
        <th>Urun Resmi</th>
        <th>Urunu Kaldır</th>   

    </tr>

    @foreach (var item in Model)
    {
                <tr>
                  <td>@item.UrunAdi</td>
                  <td>@item.UrunMiktari</td>
                  <td>@item.UrunAciklamasi</td>
                  <td><img src="@item.UrunResmi" style="width:50px;height:50px" /></td>
                
                </tr>
    }